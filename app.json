{
  "expo": {
    "name": "qrBusinessCardsRN",
    "slug": "qrBusinessCardsRN",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ],
      "package": "com.anonymous.qrBusinessCardsRN"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "permissions": {
      "camera": {
        "description": "We need your permission to use the camera",
        "require": true
      },
      "audioRecording": {
        "description": "We need your permission to record audio",
        "require": true
      }
    }
  }
}
